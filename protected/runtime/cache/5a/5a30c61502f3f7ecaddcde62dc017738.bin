			               <div class="content text">
			               <pre class="brush:php;toolbar:false">$cr&nbsp;=&nbsp;new&nbsp;CDbCriteria;&nbsp;&nbsp;&nbsp;
//select&nbsp;查询操作&nbsp;
$cr-&gt;select&nbsp;=&nbsp;&#39;*&#39;;//默认*&nbsp;&nbsp;
$cr-&gt;select&nbsp;=&nbsp;&#39;id,name&#39;;//指定的字段&nbsp;&nbsp;
$cr-&gt;select&nbsp;=&nbsp;&#39;t.*,t.id,t.name&#39;;//连接查询时，第一个表as&nbsp;t,所以用t.*&nbsp;&nbsp;
$cr-&gt;distinct&nbsp;=&nbsp;FALSE;&nbsp;//是否唯一查询&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
//join&nbsp;&nbsp;连表操作
$cr-&gt;join&nbsp;=&nbsp;&#39;left&nbsp;join&nbsp;table2&nbsp;t2&nbsp;on(t.id=t2.tid)&#39;;&nbsp;
$cr-&gt;with&nbsp;=&nbsp;&#39;xxx&#39;;&nbsp;//调用relations&nbsp;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
//where&nbsp;查询数字字段&nbsp;&nbsp;
$cr-&gt;addCondition(&quot;id=1&quot;);&nbsp;//查询条件，即where&nbsp;id&nbsp;=&nbsp;1&nbsp;&nbsp;&nbsp;&nbsp;
$cr-&gt;addBetweenCondition(&#39;id&#39;,&nbsp;1,&nbsp;4);//between&nbsp;1&nbsp;and&nbsp;4&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
$cr-&gt;addInCondition(&#39;id&#39;,&nbsp;array(1,2,3,4,5));&nbsp;//代表where&nbsp;id&nbsp;IN&nbsp;(1,23,,4,5,);&nbsp;&nbsp;&nbsp;&nbsp;
$cr-&gt;addNotInCondition(&#39;id&#39;,&nbsp;array(1,2,3,4,5));//与上面正好相法，是NOT&nbsp;IN&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
//where&nbsp;查询字符串字段&nbsp;&nbsp;
$cr-&gt;addSearchCondition(&#39;name&#39;,&nbsp;&#39;分类&#39;);//搜索条件，其实代表了。。where&nbsp;name&nbsp;like&nbsp;&#39;%分类%&#39;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;&nbsp;
//where&nbsp;查询日期字段&nbsp;&nbsp;
$cr-&gt;addCondition(&quot;create_time&gt;&#39;2012-11-29&nbsp;00:00:00&#39;&quot;);&nbsp;&nbsp;
$cr-&gt;addCondition(&quot;create_time&lt;&#39;2012-11-30&nbsp;00:00:00&#39;&quot;);&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
//where&nbsp;and&nbsp;or&nbsp;&nbsp;
$cr-&gt;addCondition(&#39;id=1&#39;,&#39;OR&#39;);//这是OR条件，多个条件的时候，该条件是OR而非AND&nbsp;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
//这个方法比较特殊，他会根据你的参数自动处理成addCondition或者addInCondition，&nbsp;&nbsp;
//即如果第二个参数是数组就会调用addInCondition&nbsp;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
$cr-&gt;compare(&#39;id&#39;,&nbsp;1);&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
/**&nbsp;&nbsp;*&nbsp;传递参数&nbsp;*/&nbsp;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
$cr-&gt;addCondition(&quot;id&nbsp;=&nbsp;:id&quot;);&nbsp;&nbsp;&nbsp;&nbsp;
$cr-&gt;params[&#39;:id&#39;]=1;&nbsp;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
//order&nbsp;&nbsp;&nbsp;
$cr-&gt;order&nbsp;=&nbsp;&#39;xxx&nbsp;DESC,XXX&nbsp;ASC&#39;&nbsp;;//排序条件&nbsp;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
//group&nbsp;&nbsp;
$cr-&gt;group&nbsp;=&nbsp;&#39;group&nbsp;条件&#39;;&nbsp;&nbsp;&nbsp;&nbsp;
$cr-&gt;having&nbsp;=&nbsp;&#39;having&nbsp;条件&nbsp;&#39;;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;
&nbsp;&nbsp;
//limit&nbsp;&nbsp;
$cr-&gt;limit&nbsp;=&nbsp;10;&nbsp;&nbsp;&nbsp;&nbsp;//取1条数据，如果小于0，则不作处理&nbsp;&nbsp;&nbsp;&nbsp;
$cr-&gt;offset&nbsp;=&nbsp;1;&nbsp;&nbsp;&nbsp;//两条合并起来，则表示&nbsp;limit&nbsp;10&nbsp;offset&nbsp;1,或者代表了。limit&nbsp;1,10</pre><p><br/></p>			           </div>
